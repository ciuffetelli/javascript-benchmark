  },
  "homepage": "https://github.com/ciuffetelli/javascript-benchmark#readme",
  "dependencies": {
    "typescript": "^5.4.5"
  },
  "types": "/types.d.ts",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.3",
    "tsconfig-paths": "^4.2.0"
